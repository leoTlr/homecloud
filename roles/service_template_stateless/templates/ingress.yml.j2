---
apiVersion: v1
kind: Ingress
metadata:
  name: '{{ app_name }}'
  namespace: '{{ app_namespace }}'
spec:
  rules:
    - host: '{{ app_name }}.{{ homecloud_fqdn }}'
      http:
        paths:
          - backend:
            serviceName: '{{ app_name }}'
            servicePort: '{{ app_port }}'
