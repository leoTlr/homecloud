---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: '{{ app_name }}'
  namespace: '{{ app_namespace }}'
spec:
  rules:
  - host: '{{ app_name }}.{{ homecloud_fqdn }}'
    http:
      paths:
      - pathType: Prefix
        path: '/'
        backend:
          service:
            name: '{{ app_name }}'
            port: 
              number: {{ app_port }}
